<!DOCTYPE html>
<html>
<head>

    <meta charset="UTF-8" />
    <title>{% block title %}Welcome{% endblock %}</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">

    <link href="{{ asset('bundles/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/plugins/daterangepicker/daterangepicker-bs3.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/plugins/jvectormap/jquery-jvectormap-1.2.2.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/plugins/morris/morris.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/plugins/iCheck/flat/blue.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/plugins/datepicker/datepicker3.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/dist/css/skins/_all-skins.min.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/dist/css/AdminLTE.min.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/bootstrap/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/plugins/fullcalendar/fullcalendar.css') }}" type="text/css" rel="stylesheet" />
    {% block cssblock %}

    {% endblock %}



    {% block script %}
    {% endblock script %}

</head>

<body class="hold-transition skin-blue sidebar-mini" >
{% block body %}

{% endblock %}

{% block content %}
<div class="wrapper">

    <header class="main-header">
        <!-- Logo -->
        <a  class="logo">
            <!-- mini logo for sidebar mini 50x50 pixels -->
            <span class="logo-mini"><b>M</b>Sp</span>
            <!-- logo for regular state and mobile devices -->
            <span class="logo-lg"><b>MoraSpirit</b></span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>
            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">

                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <img src="{{ asset('bundles/dist/img/'~app.user.email~'.jpg') }}" class="user-image" alt="User Image">
                            <span class="hidden-xs">{{ app.user.member.getFirstName }}</span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li class="user-header">
                                <img src="{{ asset('bundles/dist/img/'~app.user.email~'.jpg') }}"  alt="User Image" class="img-circle" />
                                <p>
                                    {% block username2 %}
                                        {{ app.user.member.getFirstName }} {{ app.user.member.getLastName }}
                                    {% endblock %}
                                    <small>Member since {{ app.user.member.getRegisterDate }}</small>
                                </p>
                            </li>
                            <!-- Menu Body -->
                            <li class="user-body">
                                <div class="row">
                                    <div class="col-xs-4 text-center">
                                        <a href="#">Followers</a>
                                    </div>
                                    <div class="col-xs-4 text-center">
                                        <a href="#">Sales</a>
                                    </div>
                                    <div class="col-xs-4 text-center">
                                        <a href="#">Friends</a>
                                    </div>
                                </div>
                                <!-- /.row -->
                            </li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a href="#" class="btn btn-default btn-flat">Profile</a>
                                </div>
                                <div class="pull-right">
                                    <a href="{{ path("security_logout") }}" class="btn btn-default btn-flat">Sign out</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                    <!-- User Account: style can be found in dropdown.less -->


                </ul>
            </div>
        </nav>
    </header>
    <!-- Left side column. contains the logo and sidebar -->
    <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
            <!-- Sidebar user panel -->
            <div class="user-panel">
                <div class="pull-left image">

                    <img src="{{ asset('bundles/dist/img/'~ app.user.username ~'.jpg') }}" alt="User Image" class="img-circle" />
                </div>
                <div class="pull-left info">
                    {% block username %}
                        <p>
                            {{ app.user.username }}
                        </p>
                    {% endblock  %}
                    <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                </div>
            </div>

            <!-- /.search form -->
            <!-- sidebar menu: : style can be found in sidebar.less -->
            <ul class="sidebar-menu">
                <li class="header">MAIN NAVIGATION</li>
                <li class="active treeview">
                    <a href="#">
                        <i class="fa fa-dashboard"></i> <span>Home</span> <i class="fa fa-angle-left pull-right"></i>
                    </a>
                    <ul class="treeview-menu">
                        <li class="active"><a href="{{ path('user_profile') }}"><i class="fa fa-circle-o"></i> Profile</a></li>
                        <li><a><i class="fa fa-circle-o"></i> Timeline</a></li>
                    </ul>
                </li>

                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-laptop"></i>
                        <span>Action</span>
                        <i class="fa fa-angle-left pull-right"></i>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="{{ path('reserve') }}"><i class="fa fa-circle-o"></i> Reserve Resource</a></li>
                        <li><a href="pages/UI/buttons.html"><i class="fa fa-circle-o"></i>Apply For Tranning</a></li>
                        <li><a href="pages/UI/sliders.html"><i class="fa fa-circle-o"></i> Apply For Event</a></li>
                    </ul>
                </li>
                {% if 'ROLE_ADMIN' in app.user.roles  %}
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-files-o"></i>
                            <span>Registration</span>
                            <span class="label label-primary pull-right">4</span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/student_registration"><i class="fa fa-circle-o"></i>Student Registration</a></li>
                            <li><a href="/instructor_registration"><i class="fa fa-circle-o"></i>Instructor Registration</a></li>
                            <li><a href="/resource_registration"><i class="fa fa-circle-o"></i>New Resource Registration </a></li>

                        </ul>
                    </li>

                {% endif %}
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-files-o"></i>
                        <span>Sports</span>
                        <span class="label label-primary pull-right">4</span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="pages/layout/top-nav.html"><i class="fa fa-circle-o"></i> Time Table</a></li>


                    </ul>
                </li>


                <li>
                    <a href="pages/widgets.html">
                        <i class="fa fa-th"></i>
                        <span>Events</span>
                        <small class="label pull-right bg-green">new</small>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="pages/charts/chartjs.html"><i class="fa fa-circle-o"></i>Create New Event</a></li>
                        <li><a ><i class="fa fa-circle-o"></i>Current Event</a></li>
                        <li><a><i class="fa fa-circle-o"></i>Privious Event</a></li>
                    </ul>
                </li>

                <li>
                    <a href="{{ path("calendar") }}">
                        <i class="fa fa-calendar"></i> <span>Calendar</span>
                    </a>
                </li>


            </ul>
        </section>
        <!-- /.sidebar -->
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Dashboard
                <small>Control panel</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Dashboard</li>
            </ol>

            {% block profileroot %}

            {% endblock %}

            {% block root %}

            {% endblock %}
        </section>
        {% block calendar %}

        {% endblock %}

     </div>

    {% endblock %}

    {% block javascripts %}

        <script src="{{ asset('bundles/plugins/jQuery/jQuery-2.1.4.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/plugins/slimScroll/jquery.slimscroll.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/plugins/fastclick/fastclick.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/dist/js/app.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/dist/js/demo.js') }}" type="text/javascript"></script>

        <!-- InputMask -->
        <script src="{{ asset('bundles/plugins/input-mask/jquery.inputmask.js') }}" ></script>
        <script src="{{ asset('bundles/plugins/input-mask/jquery.inputmask.date.extensions.js') }}" ></script>
        <script src="{{ asset('bundles/plugins/input-mask/jquery.inputmask.extensions.js') }}" ></script>




        <!-- date-range-picker -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.min.js"></script>
        <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>

        <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>

        <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.2/moment.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.14/daterangepicker.min.js"></script>

        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

        <script>
            $.widget.bridge('uibutton', $.ui.button);
        </script>



    {% endblock %}
    {% block javascript2 %}

    {% endblock %}

</body>
</html>









